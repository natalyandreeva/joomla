<?xml version="1.0" encoding="utf-8"?>
<form>
	<fields name="jform">
		<fieldset name="general">
			<field name="language"
				type="CsviVirtuemartLanguage"
				class="advancedSelect"
				size="1" />
		</fieldset>

		<fieldset name="product">
			<field name="update_based_on"
				type="text"
				default="product_sku" />

			<field name="unpublish_before_import"
				type="list"
				size="1"
				class="advancedSelect"
				default="0">
				<option value="1">JYES</option>
				<option value="0">JNO</option>
			</field>
		</fieldset>
	</fields>
</form>
